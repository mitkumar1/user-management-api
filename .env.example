# Jenkins Environment Configuration
# Copy this to .env and update values for your environment

# Database Configuration
DB_USER=your_db_user
DB_PASSWORD=your_secure_db_password

# Security Configuration
JWT_SECRET=your-super-secure-jwt-secret-key-at-least-32-characters-long

# Monitoring Configuration
GRAFANA_PASSWORD=your_grafana_admin_password

# Notification Configuration
NOTIFICATION_EMAIL=your-team@company.com

# Docker Registry Configuration
DOCKER_REGISTRY=your-registry.com
DOCKER_REGISTRY_CREDENTIALS=jenkins-docker-credentials

# SonarQube Configuration
SONAR_HOST_URL=http://your-sonarqube-server:9000
SONAR_TOKEN=your_sonar_token

# Deployment Configuration
STAGING_SERVER=staging.yourdomain.com
PRODUCTION_SERVER=prod.yourdomain.com

# SSL Configuration
SSL_CERT_PATH=/path/to/ssl/cert
SSL_KEY_PATH=/path/to/ssl/key
